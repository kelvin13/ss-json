{
    "name": "swift-json-container",
    "dockerComposeFile": "docker-compose.yml",
    "service": "swift",
    "workspaceFolder": "/swift-json",
    "remoteUser": "ec2-user",
    "extensions": 
    [
      "sswg.swift-lang"
    ],
    "settings": 
    {
        // needed due to https://github.com/swift-server/vscode-swift/issues/365
